<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="INFO">
    <Appenders>
        <StartUp name="StartUp" target="SYSTEM_OUT">
            <PatternLayout
                    pattern="[%-5level] MAIN %d{yyyy-MM-dd HH:mm:ss.SSS} [%t] %c{1} - %msg%n" />
        </StartUp>
    </Appenders>
    <Loggers>
        <Root level="debug" additivity="false">
            <AppenderRef ref="StartUp" />
        </Root>
    </Loggers>
</Configuration>